{
  "version": 2,
  "name": "frostbyte",
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "api/frost.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/uv/(.*)",
      "dest": "/public/uv/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/frost.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
